<div class="registration_form">
	<div class="_reg_hd_name">
		Upload Profile Image
	</div>
	<div class="inlineparent __center">
		<div class="prf_prf_pic inlineparent">
			<form action="/gb/srvc/upload-profile-pic" method="post" enctype="multipart/form-data">
				<input type="hidden" value="" name="gbId"/>
				<div class="prf_prs_pic_img">
					<img src="../img/profile_pic.png">
				</div>		
				<input type="file" name"profilepic" />
				<input type="submit">				
				<div class="prf_prs_pic_opt inlineparent">
					<div class="prs _opt_img upload" style="margin:0">Upload</div>
					<div class="prs _opt_img rmv">Remove</div>
				</div>
			</form>	
		</div>
		<div class="prf_cvr_pic inlineparent">
			<form action="/gb/srvc/upload-header-pic" method="post" enctype="multipart/form-data">
				<input type="hidden" value="" name="gbId"/>
				<div class="prf_cvr_pic_img">
					<img src="../img/defautl_cover.png">
				</div>
				<input type="file" name"profilepic" />
				<input type="submit">		
				<div class="prf_cvr_pic_opt inlineparent">
					<div class="cvr _opt_img upload" style="margin:0">Upload</div>
					<div class="cvr _opt_img rmv">Remove</div>
				</div>
			</form>	
		</div>
	</div>
	
</div>
