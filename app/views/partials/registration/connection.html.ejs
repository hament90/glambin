	<script type="type/javascript">
		$(document).ready(function(){
			$(".saveData").click(function(e){
				var _self=$(this);
				var _parentForm=_self.parents("form");
				var generalValidator = new FormValidator(_parentForm.attr("name"), [{
				    name: 'google',
				    display: 'Google',
				    rules: 'valid_url'
				},{
				    name: 'yahoo',
				    display: 'Yahoo',
				    rules: 'valid_url'
				},{
				    name: 'rediff',
				    display: 'Rediffmail',
				    rules: 'valid_url'
				},{
				    name: 'fb',
				    display: 'Facebook',
				    rules: 'valid_url'
				},{
				    name: 'twitter',
				    display: 'Twitter',
				    rules: 'valid_url'
				},{
				    name: 'hotmail',
				    display: 'My Space',
				    rules: 'valid_url'
				},{
				    name: 'tumblr',
				    display: 'Tumblr',
				    rules: 'valid_url'
				},{
				    name: 'linkedin',
				    display: 'LinkedIn',
				    rules: 'valid_url'
				},{
				    name: 'instagram',
				    display: 'Instagram',
				    rules: 'valid_url'
				},{
				    name: 'skype',
				    display: 'Skype',
				    rules: 'valid_url'
				}], function(errors, event) {
					$(".error").hide();
					console.log(errors)
					if(errors.length>0){
						for(var i=0; i<errors.length;i++){
					    	var err=$("input[name="+errors[i].name+"]").parents(".inp_blck").find('.error');
					    	err.show();
					    	err.find(".error_txt").text(errors[i].message);
				    	}
				    	return false;
					}
				    return errors.length;
				});
				var error=generalValidator._validateForm(e);
				_parentForm.ajaxSubmit(function(data){
					if(data.status==0){
						_mainForm.find(".buttonBars").find(".successMsg").remove();
						_mainForm.find(".buttonBars").append("<div class='pos_abs successMsg'>Thanks for updating yur information</div>");
						setTimeout(function(){
							_mainForm.find(".buttonBars").find(".successMsg").hide()		
						},2000)
					}
				});
			});
		});
	</script>
